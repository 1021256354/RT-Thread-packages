
config PKG_USING_SQLITE
    bool "a sqlite package"
    default n

if PKG_USING_SQLITE

    config PKG_SQLITE_PATH
        string
        default "/packages/system/sqlite"

    select PKG_SQLITE_PATH

    config PKG_SQLITE_VER
        string
        default "v3.19.3"

    select PKG_SQLITE_VER

endif
